{
    "name": "@plurid/deserve-client",
    "version": "0.0.0-0",
    "description": "Client for Deserve",
    "keywords": [
        "deserve",
        "client"
    ],
    "author": "ly3xqhl8g9 <ly3xqhl8g9@plurid.com> (https://plurid.com)",
    "license": "SEE LICENSE IN LICENSE",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/deserve"
    },
    "bugs": {
        "email": "source@plurid.com",
        "url": "https://github.com/plurid/deserve/issues"
    },
    "homepage": "https://github.com/plurid/deserve/tree/master/packages/utilities/deserve-clients/javascript",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "engines": {
        "node": ">=12",
        "npm": ">=6"
    },
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "typings": "distribution/index.d.ts",
    "files": [
        "distribution"
    ],
    "scripts": {
        "clean": "rimraf ./distribution",
        "lint": "eslint ./source --ext .ts,.tsx",
        "test": "jest -c configurations/jest.config.js ./source",
        "runs": "runner",
        "runner": "runner tests",
        "start": "node distribution/index.js",
        "start.local": "deon environment ./environment/.env.local.deon -- node distribution/index.js",
        "build.development": "yarn clean && rollup -c ./scripts/rollup.config.js --environment ENV_MODE:local",
        "build.production": "rollup -c ./scripts/rollup.config.js --environment ENV_MODE:production",
        "build.removeTests": "rimraf `find ./distribution/ -type d -name __tests__`",
        "build": "yarn clean && yarn build.production && yarn build.removeTests",
        "prepublishOnly": "yarn build"
    },
    "peerDependencies": {
        "@apollo/client": ">=3.4",
        "axios": ">=0.21",
        "cross-fetch": ">=3.1",
        "form-data": ">=4.0",
        "graphql": ">=15.5"
    },
    "devDependencies": {
        "@apollo/client": "3.4.11",
        "@plurid/runner": "^0.0.0-9",
        "@types/jest": "^27.0.1",
        "@types/node": "^16.9.4",
        "@typescript-eslint/eslint-plugin": "^4.31.1",
        "@typescript-eslint/parser": "^4.31.1",
        "@zerollup/ts-transform-paths": "^1.7.18",
        "axios": "^0.21.4",
        "cross-fetch": "^3.1.4",
        "eslint": "^7.32.0",
        "form-data": "^4.0.0",
        "graphql": "^15.5.3",
        "jest": "^27.2.0",
        "rimraf": "^3.0.2",
        "rollup": "^2.56.3",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.30.0",
        "ts-jest": "^27.0.5",
        "ts-node": "^10.2.1",
        "ttypescript": "^1.5.12",
        "typescript": "^4.4.3",
        "typescript-transform-paths": "^3.3.1"
    }
}
