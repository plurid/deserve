{
    "name": "@plurid/deserve-router",
    "version": "0.0.0-0",
    "description": "Owner Data Deserver Router",
    "keywords": [
        "deserve",
        "router"
    ],
    "author": "ly3xqhl8g9 <ly3xqhl8g9@plurid.com> (https://plurid.com)",
    "license": "SEE LICENSE IN LICENSE",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/deserve"
    },
    "bugs": {
        "email": "source@plurid.com",
        "url": "https://github.com/plurid/deserve/issues"
    },
    "homepage": "https://github.com/plurid/deserve/blob/master/packages/deserve-router/README.md",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "engines": {
        "node": ">=12",
        "npm": ">=6"
    },
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "typings": "distribution/index.d.ts",
    "files": [
        "distribution"
    ],
    "scripts": {
        "clean": "rm -rf ./distribution",
        "lint": "eslint ./source --ext .ts,.tsx",
        "start": "deon environment ./environment/.env.local.deon -- node test/index.js",
        "build.development": "yarn clean && rollup -c ./scripts/rollup.config.js --environment ENV_MODE:local",
        "build.production": "rollup -c ./scripts/rollup.config.js --environment ENV_MODE:production",
        "build.removeTests": "rimraf `find ./distribution/ -type d -name __tests__`",
        "build": "yarn clean && yarn build.production && yarn build.removeTests",
        "prepublishOnly": "yarn build"
    },
    "dependencies": {
        "@plurid/delog": "^0.0.0-4",
        "body-parser": "^1.19.0",
        "express": "^4.17.1"
    },
    "devDependencies": {
        "@plurid/deon": "^0.0.0-4",
        "@types/body-parser": "^1.19.1",
        "@types/express": "^4.17.13",
        "@types/node": "^15.12.2",
        "@types/react": "^17.0.11",
        "@typescript-eslint/eslint-plugin": "^4.27.0",
        "@typescript-eslint/parser": "^4.27.0",
        "@zerollup/ts-transform-paths": "^1.7.18",
        "eslint": "^7.28.0",
        "react": "^17.0.2",
        "rimraf": "^3.0.2",
        "rollup": "^2.51.2",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.30.0",
        "ts-node": "^10.0.0",
        "ttypescript": "^1.5.12",
        "typescript": "^4.3.2",
        "typescript-transform-paths": "^2.2.4"
    }
}
