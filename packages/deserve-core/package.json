{
    "name": "@plurid/deserve-core",
    "version": "0.0.0-3",
    "description": "Deserve Core to Own Your Data",
    "keywords": [
        "deserve",
        "core"
    ],
    "author": "ly3xqhl8g9 <ly3xqhl8g9@plurid.com> (https://plurid.com)",
    "license": "SEE LICENSE IN LICENSE",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/deserve"
    },
    "bugs": {
        "email": "source@plurid.com",
        "url": "https://github.com/plurid/deserve/issues"
    },
    "homepage": "https://github.com/plurid/deserve/blob/master/packages/deserve-core/README.md",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "engines": {
        "node": ">=12",
        "npm": ">=6"
    },
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "typings": "distribution/index.d.ts",
    "files": [
        "distribution"
    ],
    "scripts": {
        "clean": "rimraf ./distribution",
        "lint": "eslint ./source --ext .ts,.tsx",
        "start": "deon environment ./environment/.env.local.deon -- node test/index.js",
        "build.development": "pnpm clean && rollup -c ./scripts/rollup.config.js --environment ENV_MODE:local",
        "build.production": "rollup -c ./scripts/rollup.config.js --environment ENV_MODE:production",
        "build.removeTests": "rimraf `find ./distribution/ -type d -name __tests__`",
        "build.copyAssets": "cp -r ./source/assets ./distribution/assets",
        "build": "pnpm clean && pnpm build.production && pnpm build.removeTests && pnpm build.copyAssets",
        "prepublishOnly": "pnpm build"
    },
    "dependencies": {
        "@plurid/delog": "^0.0.0-6",
        "@plurid/plurid-functions": "^0.0.0-30",
        "body-parser": "^1.20.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "express": "^4.18.1",
        "pump": "^3.0.0"
    },
    "devDependencies": {
        "@plurid/deon": "^0.0.0-8",
        "@types/body-parser": "^1.19.2",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jest": "^29.0.0",
        "@types/node": "^18.7.15",
        "@types/pump": "^1.1.1",
        "@typescript-eslint/eslint-plugin": "^5.36.2",
        "@typescript-eslint/parser": "^5.36.2",
        "@zerollup/ts-transform-paths": "^1.7.18",
        "eslint": "^8.23.0",
        "jest": "^29.0.2",
        "rimraf": "^3.0.2",
        "rollup": "^2.79.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.33.0",
        "ts-jest": "^28.0.8",
        "ts-node": "^10.9.1",
        "tslib": "^2.4.0",
        "ttypescript": "^1.5.13",
        "typescript": "^4.8.2",
        "typescript-transform-paths": "^3.3.1"
    }
}
